    "def max_min(li):\n",
    "  max, min = li.pop(), li.pop()\n",
    "  for i in li:\n",
    "    if i > max:\n",
    "      max = i\n",
    "    if i < min:\n",
    "      min = i\n",
    "\n",
    "  return max, min\n",
    "\n",
    "li = [1, 52, 273, 32, 99, 101]\n",
    "\n",
    "print(max_min(li))"
   ]
  }
